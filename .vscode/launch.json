  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run project",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "watch"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "DEBUG": "db|remote",
        "NODE_ENV": "development"
      }
    }
  ]
}